{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
		"baseUrl": "./",
		"paths": {
			"@/*": ["./frontend/*"]
		}
  },
	"exclude": [
		"node_modules",
		".next",
		"dist",
		"build"
	]
}
